package com.staragile.logics;

public class Demo1For {
	
	public static void main(String[] args) {
		
		//1 to 10
		//start point
		//end point 
		//increment 
		//(i++)  i=i+1
		String[] arrs= {"red","green","blue"};
		
		for(int i=0;i<arrs.length;i++)
		{
			System.out.println(arrs[i]);
		}
		
		
	}

}
